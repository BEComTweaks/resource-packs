      "obvious_conflict": [
        "SmoothFont",
        "AntiAliasFont",
        "SmallCapsFont",
        "SquareFont",
        "BlockyFont"
      ]
    },
    {
      "pack_id": "BlockyFont",
      "pack_name": "Blocky Font",
      "pack_description": "Changes the font to be bold and blocky, from the 'F8thful' resource pack",
      "obvious_conflict": [
        "SmoothFont",
        "AntiAliasFont",
        "SmallCapsFont",
        "SquareFont",
        "DoodleFont"
      ]
    }
  ]
