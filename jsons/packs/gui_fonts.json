      "obvious_conflict": [
        "SmoothFont",
        "AntiAliasFont",
        "SmallCapsFont",
        "DoodleFont",
        "BlockyFont"
      ]
    },
    {
      "pack_id": "DoodleFont",
      "pack_name": "Doodle Font",
